<!-- Content styles -->
<style>
 
 @font-face {
   font-family:HarmonyOS_Sans_SC_Medium;font-style: normal;font-display: swap;
   src: url('/fonts/HarmonyOS_Sans_SC_Medium.woff2') format('woff2'),
   url('/fonts/HarmonyOS_Sans_SC_Medium.woff') format('woff');
 }
 
 *{
   font-family:HarmonyOS_Sans_SC_Medium;
 }
 
 html{
   font-size: 11px;
 }
 

 body {
   background-color: #f0fcff;
   color: #161823;
   padding-top: 4.5rem;
   margin: 0;
 }
 

 .rwd-small-font-color{
   font-size: 5px;
   color: #adb5bd
 }

 .rwd-small-img{
   max-width: 100%;
   height: auto;
 }

 h2.rwd-whispers-header{
   text-align: center;
 }

 .rwd-whispers-text{

 }
 
 h5{
   color: #adb5bd;
 }

 p {

   text-indent: 4px;
 }

 /* unvisited link */
a:link {
  color: #44cef6;
  text-decoration: none;
  
}

/* visited link */
a:visited {
  color: #000000;
  text-decoration: none;
}

/* mouse over link */
a:hover {
    color: #44cef6;
}

/* selected link */
a:active {
    color: #00bc12;
}
</style>

