<!-- Content styles -->
<style>

 *{
   margin: 0;
   padding: 0;
   font-family: 'Roboto', sans-serif;
 }
 
 body {
   color: #202013;
   font-weight: 350;
   line-height: 1.628;
   letter-spacing: -.003em;
   font-size: 16px;
   padding: 70px;
   background-image: url("/img/bg-00.jpg");
   background-repeat: no-repeat;
   background-size: cover;
   min-width: 100%; 
   min-height: 100vh;
 }

 $utilities: map-merge(
  $utilities,
  (
    "cursor": (
      property: cursor,
      class: cursor,
      responsive: true,
      values: auto pointer grab,
    )
  )
)

 .div-col-md-bg{
   background-image: url("/img/content-bg-01.jpg");
   background-repeat: no-repeat;
   background-size: cover;
 }

 .div-col-md-8{
   background-image: url("/img/content-bg-01.jpg");
   background-repeat: no-repeat;
   background-size: cover;
 }
 

 .rwd-small-font-color{
   font-family: 'Open Sans Condensed', sans-serif;
   font-size: 5px;
   color: #adb5bd
 }

 .rwd-small-img{
   max-width: 100%;
   height: auto;
 }

 h2.rwd-whispers-header{
   text-align: center;
 }

 .rwd-whispers-text{

 }
 
 h5{
   color: #adb5bd;
 }

 p {

   text-indent: 4px;
 }

 /* unvisited link */
a:link {
  color: #44cef6;
  text-decoration: none;
  
}

/* visited link */
a:visited {
  color: #000000;
  text-decoration: none;
}

/* mouse over link */
a:hover {
    color: #44cef6;
}

/* selected link */
a:active {
    color: #00bc12;
}
</style>

<!-- Custom styles for this template -->
